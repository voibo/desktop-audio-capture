{
  "name": "@voibo/desktop-media-capture",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.1.8",
  "author": "Voibo",
  "license": "MIT",
  "description": "Desktop audio and video capture for Node.js",
  "files": [
    "lib/",
    "include/",
    "src/",
    "CMakeLists.txt",
    "index.d.ts",
    "index.mjs",
    "index.cjs",
    "scripts/install.mjs"
  ],
  "main": "./index.cjs",
  "module": "./index.mjs",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "types": "./index.d.ts",
  "type": "module",
  "binary": {
    "napi_versions": [
      7
    ]
  },
  "dependencies": {
    "bindings": "^1.5.0"
  },
  "peerDependencies": {
    "cmake-js": "^7.3.0",
    "node-addon-api": "^8.0.0"
  },
  "scripts": {
    "install": "node scripts/install.mjs",
    "prepack": "node scripts/prepack.js",
    "postpack": "node scripts/postpack.js",
    "check_build": "npx cmake-js compile --verbose > build_log.log 2>&1"
  }
}
